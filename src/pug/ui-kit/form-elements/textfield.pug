            span.dropdown-expanded-guest__header dropdown
            input(class= 'dropdown-expanded-guest__input', type= 'text', placeholder= 'Сколько гостей')
            .dropdown-expanded-guest__icon
            .dropdown-expanded-guest__rectangle
              .row-group
                span.dropdown-expanded-guest__text взрослые 
                .guest-group.dropdown-expanded-guest__group
