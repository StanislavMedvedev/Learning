  .item02 
    .item2 
      .checkbox-buttons
        .checkbox-buttons__base-form
          .checkbox-buttons__text Checkbox Buttons
            .checkbox-buttons__group
              label
                input(class= 'checkbox-buttons__group-input', type= 'checkbox', value= 'smoke', name= 'smoke')
                span.checkbox-buttons__group-text Можно курить
            .checkbox-buttons__group
              label
                input(class= 'checkbox-buttons__group-input', type= 'checkbox', value= 'pet', name= 'pet', checked)
                span.checkbox-buttons__group-text Можно с питомцами
            .checkbox-buttons__group
              label
                input(class= 'checkbox-buttons__group-input', type= 'checkbox', value= 'guest', name= 'guest', checked)
                span.checkbox-buttons__group-text Можно пригласить гостей (до 10 человек)
    .item2
      .radio-button-wrapper
        .radio-button__base-form 
          span.radio-button__header Radio Buttons
          .radio-button
            label
              input(type='radio', class='radio-button__circle', name='radio-button', checked)
              span(class='radio-button__text radio-button__text--moved-right component__text') Мужчина
            label
              input(type='radio', class='radio-button__circle', name='radio-button')
              span(class='radio-button__text component__text') Женщина
    .item2
      .toggle-button__base-form 
        span.toggle-button__header Toggle
          .toggle-container
            .toggle-button
              label
                input(type='checkbox', class='toggle-button__input', checked)
                span.toggle-button__text Получать спецпредложения
          .toggle-container
            .toggle-button
              label
                input(type='checkbox', class='toggle-button__input')
                span.toggle-button__text Получать спецпредложения
    .item2
      .like-button
        +text({main: 'like-button__text', title: 'like button'})
        .content
          input(type='checkbox', class='like-button__rectangle')
          .like-button__heart
          .like-button__count_uncolored 2
          .like-button__count_colored 2
        .content-unchecked
          input(type='checkbox', class='like-button__rectangle', checked)
          .like-button__heart
          .like-button__count_uncolored 12
          .like-button__count_colored 12
    .item2
      .rate-button
        +text({main: 'rate-button__text', title: 'rate button'})
        fieldset.stars
        .rating_stars
          input(class= 'rating_star' type='radio', value='1', name= '1star')
          input(class= 'rating_star' type='radio', value='2', name= '2star')
          input(class= 'rating_star' type='radio', value='3', name= '3star')
          input(class= 'rating_star' type='radio', value='4', name= '4star', checked)
          input(class= 'rating_star' type='radio', value='5', name= '5star')
        .star-5-checked
          fieldset.stars
          .rating_stars
            input(class= 'rating_star' type='radio', value='1', name= '1star')
            input(class= 'rating_star' type='radio', value='2', name= '2star')
            input(class= 'rating_star' type='radio', value='3', name= '3star')
            input(class= 'rating_star' type='radio', value='4', name= '4star')
            input(class= 'rating_star' type='radio', value='5', name= '5star', checked)
    .item2
      .dropdown-expanded-guest-wrapper
        .dropdown-expanded-guest
