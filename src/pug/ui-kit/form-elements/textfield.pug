              +checkboxForm({value: 'breakfast', name: 'breakfast', text: 'Завтрак'})
              +checkboxForm({value: 'table', name: 'table', text: 'Письменный стол', checked: 'checked'})
              +checkboxForm({value: 'chair', name: 'chair', text: 'Стул для кормления', checked: 'checked'})
              +checkboxForm({value: 'bed', name: 'bed', text: 'Кроватка', checked: 'checked'})
