        +dropdownForm({titleText: 'ванные комнаты', circleNumber: '0', circleMinusMod: 'disabled', circlePlusMod: 'moved-left'})
    .item2
      .expandable-checkbox-list-wrapper
        .expandable-checkbox-list
          .expandable-checkbox-list__base-form
            span.expandable-checkbox-list__text expandable checkbox list
            .expandable-checkbox-list__icon
    .item2
      .expandable-checkbox-list-wrapper1
        .expandable-checkbox-list
          .expandable-checkbox-list__base-form.expandable-checkbox-list__base-form--large
            span.expandable-checkbox-list__text expandable checkbox list
